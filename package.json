{
  "name": "discord-music-bot",
  "version": "4.2.0",
  "description": "Simple Music Bot for Discord without any database",
  "private": true,
  "engines": {
    "node": ">=16.16.0"
  },
  "scripts": {
    "test": "mocha",
    "start": "npm run build && npm run onlystart",
    "onlystart": "node util/exec dist",
    "build": "npm run cleanup && node builder.mjs bake && tsc -p util/tsconfig/tsconfig.build.json",
    "build:bundled": "npm run cleanup && node builder.mjs bake && tsc -p util/tsconfig/tsconfig.bundle.json && node builder.mjs bundle",
    "prelint": "node builder.mjs bake",
    "lint": "tsc && eslint .",
    "cleanup": "rimraf dist/ typings/ build/",
    "generatecommandlist": "node util/generateCommandList"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.1",
    "@mtripg6666tdr/async-lock": "^1.1.0",
    "@mtripg6666tdr/oceanic-command-resolver": "~1.4.1",
    "@sinclair/typebox": "^0.32.25",
    "candyget": "^0.5.6",
    "comment-json": "^4.2.3",
    "dotenv": "^16.4.5",
    "genius-lyrics": "^4.4.7",
    "html-entities": "^2.5.2",
    "html-to-text": "^9.0.5",
    "https-proxy-agent": "^7.0.4",
    "i18next": "^23.11.2",
    "i18next-fs-backend": "^2.3.1",
    "log4js": "^6.9.1",
    "miniget": "^4.2.3",
    "niconico-dl.js": "^1.1.3",
    "oceanic.js": "~1.10.2",
    "p-event": "^4.2.0",
    "p-queue": "^6.6.2",
    "prism-media": "^1.3.5",
    "soundcloud.ts": "^0.5.2",
    "spotify-url-info": "^3.2.13",
    "throttle-debounce": "^5.0.0",
    "tslib": "^2.6.2",
    "undici": "^5.28.4",
    "yt-mix-playlist": "^1.0.1",
    "ytdl-core": "^4.11.5",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "@mtripg6666tdr/eslint-config": "^3.0.16",
    "@swc/core": "^1.5.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/html-to-text": "^9.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "@types/throttle-debounce": "^5.0.2",
    "@types/ws": "^8.5.10",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-plugin-license-header": "^0.6.1",
    "mocha": "^10.4.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.9.0",
    "@distube/ytpl": "^1.2.1",
    "@distube/ytsr": "^2.0.0",
    "erlpack": "^0.1.4",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.13",
    "mongodb": "^6.5.0",
    "opusscript": "^0.0.8",
    "pako": "^2.1.0",
    "play-dl": "^1.9.7",
